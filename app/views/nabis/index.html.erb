<p id="notice"><%= notice %></p>
<h1>Welcome to Karik team</h1>
<h2>Nabis</h2>

<table>
  <thead>
    <tr>
     
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nabis.each do |nabi| %>
      <tr>
        <td><%= nabi.name %></td>
        <td><%= nabi.description %></td>
        <td><%= nabi.price %></td>
        <td><%= link_to 'Show', nabi %></td>
        <td><%= link_to 'Edit', edit_nabi_path(nabi) %></td>
        <td><%= link_to 'Destroy', nabi, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nabi', new_nabi_path %>
